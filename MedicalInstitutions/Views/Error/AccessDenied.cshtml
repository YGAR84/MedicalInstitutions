@{
      ViewBag.Title = "Index";
      Layout = "~/Views/Shared/_Layout.cshtml";

}

<h2>Oops</h2>
<p>Access denied</p>

<img src="~/images/accessDenied.jpg" style="margin-top:20px; margin-bottom:20px;">

    @if (Model != null @*&& HttpContext.Current.IsDebuggingEnabled*@)
    {
        string controllerName = @Model.ControllerName.ToString();
        <div>
            @Html.ActionLink("Back", "Index", controllerName)
        </div>
    }
    else
    {
        <div>
            @Html.ActionLink("Back", "Index", "Home")
        </div>
    }